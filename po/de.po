#: src/conf.c:86
#, c-format
msgid "Usage: %s [OPTIONS] [DATA]"
msgstr "Verwendung: %s [OPTIONEN] [DATEN]"

#: src/conf.c:87
msgid "Options are:"
msgstr "Optionen sind:"

#: src/conf.c:88
msgid "   -f, --fullscreen      activate fullscreen"
msgstr "   -f, --fullscreen      Vollbild aktivieren"

#: src/conf.c:89
msgid "   -F n, --fps n         limit frames per second to n"
msgstr "   -F n, --fps n         Einzelbilder pro Sekunde auf n begrenzen"

#: src/conf.c:90
msgid "   -V, --vsync           enable vsync"
msgstr "   -V, --vsync           VSync aktivieren"

#: src/conf.c:91
msgid "   -W n                  set width to n"
msgstr "   -W n                  Breite auf n setzen"

#: src/conf.c:92
msgid "   -H n                  set height to n"
msgstr "   -H n                  Höhe auf n setzen"

#: src/conf.c:93
msgid "   -j n, --joystick n    use joystick n"
msgstr "   -j n, --joystick n    Joystick n benutzen"

#: src/conf.c:94
msgid "   -J s, --Joystick s    use joystick whose name contains s"
msgstr "   -J s, --Joystick s    Joystick, dessen Name „s“ enthält, benutzen"

#: src/conf.c:95
msgid "   -M, --mute            disables sound"
msgstr "   -M, --mute            Ton deaktivieren"

#: src/conf.c:96
msgid "   -S, --sound           forces sound"
msgstr "   -S, --sound           Ton erzwingen"

#: src/conf.c:97
msgid "   -m f, --mvol f        sets the music volume to f"
msgstr "   -m f, --mvol f        Musiklautstärke auf f setzen"

#: src/conf.c:98
msgid "   -s f, --svol f        sets the sound volume to f"
msgstr "   -s f, --svol f        Tonlautstärke auf f setzen"

#: src/conf.c:99
msgid "   -G, --generate        regenerates the nebula (slow)"
msgstr "   -G, --generate        den Nebel neu generieren (langsam)"

#: src/conf.c:100
msgid "   -N, --nondata         do not use ndata and try to use laid out files"
msgstr "   -N, --nondata         ndata nicht benutzen und versuchen, angelegte Dateien zu benutzen"

#: src/conf.c:101
msgid "   -d, --datapath        specifies a custom path for all user data (saves, screenshots, etc.)"
msgstr ""
"   -d, --datapath        benutzerdefinierten Pfad für alle Benutzerdaten (Spielstände, Screenshots, usw.) "
"benutzen"

#: src/conf.c:102
msgid "   -X, --scale           defines the scale factor"
msgstr "   -X, --scale           Skalierungsfaktor definieren"

#: src/conf.c:104
msgid "   --devmode             enables dev mode perks like the editors"
msgstr "   --devmode             Entwicklermodusgimmicks wie die Editoren aktivieren"

#: src/conf.c:105
msgid "   --devcsv              generates csv output from the ndata for development purposes"
msgstr "   --devcsv              CSV-Ausgabe für ndata für Entwicklungszwecke generieren"

#: src/conf.c:107
msgid "   -h, --help            display this message and exit"
msgstr "   -h, --help            diese Nachricht anzeigen und beenden"

#: src/conf.c:108
msgid "   -v, --version         print the version and exit"
msgstr "   -v, --version         Version ausgeben und beenden"

